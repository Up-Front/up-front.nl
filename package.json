{
  "name": "universal-react",
  "version": "1.0.0",
  "description": "A universal react starter, with routing, meta, title, and data features",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/DominicTobias/universal-react.git"
  },
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "express",
    "routing",
    "server",
    "redux",
    "flux"
  ],
  "author": "Dominic Tobias <dominic.tobias@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DominicTobias/universal-react/issues"
  },
  "homepage": "https://github.com/DominicTobias/universal-react",
  "scripts": {
    "start": "node server",
    "start-prod": "npm run build && NODE_ENV=production PORT=3000 node server",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "base-64": "^0.1.0",
    "cross-env": "^5.0.1",
    "del": "^3.0.0",
    "express": "^4.14.1",
    "history": "^4.5.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^20.0.4",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.5",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "webpack": "^3.2.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "react-transform-hmr": "^1.0.4"
  }
}
